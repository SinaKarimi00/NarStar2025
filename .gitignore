Lightbot/ExportedObj/
Lightbot/.consulo/
Lightbot/*.csproj
Lightbot/*.unityproj
Lightbot/*.sln
Lightbot/*.suo
Lightbot/*.tmp
Lightbot/*.user
Lightbot/*.userprefs
Lightbot/*.pidb
Lightbot/*.booproj
Lightbot/*.svd
Lightbot/*.pdb
Lightbot/*.mdb
Lightbot/*.opendb
Lightbot/*.VC.db

# Unity3D generated meta files
Lightbot/*.pidb.meta
Lightbot/*.pdb.meta
Lightbot/*.mdb.meta

# Unity3D generated file on crash reports
Lightbot/sysinfo.txt

# Builds
Lightbot/*.apk
Lightbot/*.aab
Lightbot/*.unitypackage
Lightbot/*.unitypackage.meta
Lightbot/*.app

# Crashlytics generated file
Lightbot/crashlytics-build.properties

# Packed Addressables
Lightbot/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

